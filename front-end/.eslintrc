{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
  },
  "rules": {
    "consistent-return": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "linebreak-style": 0,
    "react/forbid-prop-types": 0,
    "camelcase": 0,
    "jsx-a11y/no-static-element-interactions": 0
  },
  "plugins": [
    "import",
    "react",
    "redux-saga"
  ],
  "settings": {
    "import/resolver": "webpack"
  }
}
